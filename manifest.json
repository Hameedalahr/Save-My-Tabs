{
    "manifest_version": 3,
    "name": "Save My Tabs",
    "version": "1.0",
    "description": "Save and access your favorite tabs quickly",
    "permissions": ["storage", "tabs"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  